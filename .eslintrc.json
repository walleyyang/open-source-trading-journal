{
    "extends": "airbnb",
    "rules": {
        "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
        "jsx-a11y/label-has-associated-control": ["error", {"required": {"some": ["nesting", "id"]}}],
        "jsx-a11y/label-has-for": ["error", {"required": {"some": ["nesting", "id"]}}]
    },
    "env": {
        "browser": true,
        "node": true
    }
}